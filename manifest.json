{
    "manifest_version": 3,
    "name": "Simple CVSS Calculator Chrome Extension",
    "version": "2.0",
    "description": "Simple tool to calculate 4.0/3.1 CVSS Base Score",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "permissions": [
        
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}
